datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Guild {
  id        String   @id @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id        String   @id @unique
  role      Role     @default(NORMAL)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Role {
  NORMAL
  STAFF
  DEV
}
